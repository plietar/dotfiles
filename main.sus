card main {
    spark card common
    spark card local
}

card common {
    outof common
    into $(HOME)

    .gitconfig
    .dircolors
    .tmux.conf

    .vim
    .vimrc

    .zsh
    .zshrc

    {
        outof fish
        into .config/fish

        config.fish
        functions
    }
}

card local {
    outof $(SPARK)
    into $(HOME)

    .gitconfig.local
}

